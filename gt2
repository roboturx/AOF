##!/bin/bash
echo "***********************************************************"
echo "****************** gitt 1.3-02.2024 ***********************"
echo "***********************************************************"


for d in */ ; do

    echo "*-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-/-   $d"
